[
  {
    "repository": "rolldown/rolldown",
    "ref": "main",
    "path": "rolldown",
    "command": "./oxlint --deny-warnings -c .oxlintrc.json --ignore-path=.oxlintignore -D getter-return --import-plugin --jsdoc-plugin"
  },
  {
    "repository": "napi-rs/napi-rs",
    "ref": "main",
    "path": "napi-rs",
    "command": "./oxlint --deny-warnings --ignore-path=.oxlintignore --import-plugin -D getter-return -A no-export"
  },
  {
    "repository": "toeverything/affine",
    "ref": "canary",
    "path": "affine",
    "command": "./oxlint --deny-warnings -c oxlint.json --import-plugin -D getter-return -D perf"
  },
  {
    "repository": "preactjs/preact",
    "ref": "main",
    "path": "preact",
    "command": "./oxlint --deny-warnings -c oxlint.json -D getter-return oxlint src test debug compat hooks test-utils"
  },
  {
    "repository": "calcom/cal.com",
    "ref": "main",
    "path": "cal",
    "command": "./oxlint -D getter-return"
  },
  {
    "repository": "bbc/simorgh",
    "ref": "latest",
    "path": "simorgh",
    "command": "./oxlint -D getter-return"
  },
  {
    "repository": "DefinitelyTyped/DefinitelyTyped",
    "ref": "master",
    "path": "DefinitelyTyped",
    "command": "./oxlint -D getter-return"
  }
]
